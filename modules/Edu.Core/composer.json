{
  "name": "ibrand/edu-core",
  "type": "library",
  "description": "iBrand edu core.",
  "keywords": [
    "iBrand",
    "edu",
    "server"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shjchen",
      "email": "i@chenhow.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ibrand/discount": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "psr-4": {
